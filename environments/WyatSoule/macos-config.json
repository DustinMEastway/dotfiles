{
  "fileRoot": "./environments/WyatSoule/",
   "commands": [
  {
    "key": "brew-tap",
    "value": {
      "items": [
        "homebrew/cask-versions",
        "mongodb/brew"
      ]
    }
  },
  {
    "key": "brew-install",
    "value": {
      "items": [
        "coreutils",
        "dotnet",
        "ffmpeg",
        "mongodb-community",
        "mono",
        "postgresql",
        "redis",
        "ruby"
      ]
    }
  },
  {
    "key": "brew-install",
    "value": {
      "args": "--cask",
      "items": [
        "android-studio",
        "brave-browser",
        "curseforge",
        "discord",
        "docker",
        "gimp",
        "godot-mono",
        "insomnia",
        "mos",
        "multipatch",
        "pgadmin4",
        "private-internet-access",
        "raycast",
        "slack",
        "steam",
        "sublime-merge",
        "sublime-text",
        "zoom",
        "zulu11"
      ]
    }
  },
    {
      "key": "git-setup",
      "value": { "configPath": "./private/git/symlink.gitconfig" }
    },
    {
      "key": "mkdir",
      "value": "./private/.ssh/"
    },
    {
      "key": "ssh-keygen",
      "value": {
        "file": "./private/.ssh/id_rsa",
        "type": "rsa"
      }
    },
    {
      "key": "symlink",
      "value": [
        { "file": "./private/.ssh/", "link": "~/.ssh/" },
        { "file": "./private/git/symlink.gitconfig", "link": "~/.gitconfig" },
        { "file": "./zsh/aliases.symlink.zsh", "link": "~/.aliases.zsh" },
        { "file": "./zsh/zshrc.symlink.zsh", "link": "~/.zshrc" },
        { "file": "./private/zsh/env.symlink.zsh", "link": "~/.env.zsh" },
        { "file": "./sublime-text/user-package/", "link": "~/Library/Application Support/Sublime Text/Packages/User"}
      ]
    }
  ]
}