{
  "fileRoot": "./environments/DustinMEastway/",
  "commands": [
    {
      "key": "brew-install",
      "value": {
        "items": [
          "nvm",
          "pyenv"
        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "args": "--cask",
        "items": [
          "brave-browser",
          "insomnia",
          "raycast",
          "sublime-merge",
          "sublime-text"
        ]
      }
    },
    {
      "key": "git-setup",
      "value": { "configPath": "./private/git/symlink.gitconfig" }
    },
    { "key": "mkdir", "value": "./private/ssh/" },
    {
      "key": "ssh-keygen",
      "value": {
        "file": "./private/ssh/id_rsa",
        "type": "rsa"
      }
    },
    {
      "key": "symlink",
      "value": [
        { "file": "./work-macos/sh/aliases.symlink.zsh", "link": "~/.aliases.zsh" },
        { "file": "./sh/bin", "link": "~/.bin" },
        { "file": "./common/sh/aliases.symlink.sh", "link": "~/.common.aliases.sh" },
        { "file": "./common/sh/common.symlink.sh", "link": "~/.common.sh" },
        { "file": "./zsh/env.symlink.zsh", "link": "~/.env.zsh" },
        { "file": "./private/zsh/env.symlink.zsh", "link": "~/.env-private.zsh" },
        { "file": "./private/git/symlink.gitconfig", "link": "~/.gitconfig" },
        { "file": "./private/ssh/", "link": "~/.ssh/" },
        { "file": "./zsh/zshrc.symlink.zsh", "link": "~/.zshrc" },
        {
          "file": "./work-macos/sublime-text/user-package/",
          "link": "~/Library/Application Support/Sublime Text/Packages/User/"
        }
      ]
    }
  ]
}
