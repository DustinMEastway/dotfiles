{
  "fileRoot": "./environments/DustinMEastway/",
  "commands": [
    {
      "key": "brew-install",
      "value": {
        "items": [
          "deno",
          "mvnvm",
          "nvm",
          "pyenv"
        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "args": "--cask",
        "items": [
          "1password",
          "1password-cli",
          "brave-browser",
          "espanso",
          "insomnia",
          "obsidian",
          "raycast",
          "rider",
          "sublime-merge",
          "sublime-text"
        ]
      }
    },
    {
      "key": "git-setup",
      "value": {
        "configPath": "./private/git/symlink.gitconfig"
      }
    },
    {
      "key": "mkdir",
      "value": "./private/.ssh/"
    },
    {
      "key": "ssh-keygen",
      "value": {
        "file": "./private/.ssh/id_rsa",
        "type": "rsa"
      }
    },
    {
      "key": "write-defaults",
      "value": {
        "Apple Global Domain": {
          "ApplePressAndHoldEnabled": false
        }
      }
    },
    {
      "key": "symlink",
      "value": [
        {
          "file": "./common/sh/aliases.symlink.sh",
          "link": "~/.aliases-common.sh"
        },
        {
          "file": "./common/sh/bashrc.symlink.sh",
          "link": "~/.bashrc"
        },
        {
          "file": "./common/sh/bin/",
          "link": "~/.bin-common/"
        },
        {
          "file": "./common/sh/common.symlink.sh",
          "link": "~/.common.sh"
        },
        {
          "file": "./common/sh/env.symlink.sh",
          "link": "~/.env-common.sh"
        },
        {
          "file": "./common/sh/zshrc.symlink.zsh",
          "link": "~/.zshrc"
        },
        {
          "file": "./common/espanso/",
          "link": "~/Library/Application Support/espanso/"
        },
        {
          "file": "./common/sublime-merge/",
          "link": "~/Library/Application Support/Sublime Merge/Packages/User/"
        },
        {
          "file": "./common/sublime-text/",
          "link": "~/Library/Application Support/Sublime Text/Packages/User/"
        },
        {
          "file": "./work-macos/sh/aliases.symlink.sh",
          "link": "~/.aliases.sh"
        },
        {
          "file": "./work-macos/sh/env.symlink.sh",
          "link": "~/.env.template.sh"
        },
        {
          "file": "./work-macos/sublime-text/Preferences.sublime-settings",
          "link": "./common/sublime-text/Preferences.sublime-settings"
        },
        {
          "file": "./work-macos/sublime-text/projects/",
          "link": "./common/sublime-text/projects/"
        },
        {
          "file": "./work-macos/sublime-text/snippets/",
          "link": "./common/sublime-text/snippets/"
        },
        {
          "file": "./private/.ssh/",
          "link": "~/.ssh/"
        },
        {
          "file": "./private/git/symlink.gitconfig",
          "link": "~/.gitconfig"
        },
        {
          "file": "./private/sh/env.symlink.sh",
          "link": "~/.env-private.sh"
        }
      ]
    },
    {
      "key": "brew-sync"
    }
  ]
}
