{
  "fileRoot": "./environments/DustinMEastway/",
  "commands": [
    {
      "key": "brew-tap",
      "value": {
        "items": [
          "mongodb/brew"
        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "items": [
          "coreutils",
          "ffmpeg",
          "mongodb-community",
          "postgresql",
          "ruby",
          "redis"

        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "args": "--cask",
        "items": [
          "altserver",
          "bfxr",
          "brave-browser",
          "calibre",
          "curseforge",
          "docker",
          "godot-mono",
          "iina",
          "insomnia",
          "multipatch",
          "pgadmin4",
          "sublime-merge",
          "zoom"
        ]
      }
    },
    {
      "key": "git-setup",
      "value": { "configPath": "./private/git/symlink.gitconfig" }
    },
    { "key": "mkdir", "value": "./private/.ssh/" },
    { "key": "mkdir", "value": "./private/sh/bin/" },
    {
      "key": "ssh-keygen",
      "value": {
        "file": "./private/.ssh/id_rsa",
        "type": "rsa"
      }
    },
    {
      "key": "symlink",
      "value": [
        { "file": "../..", "link": "~/.dotfiles" },
        { "file": "./common.symlink.sh", "link": "~/.common.sh" },
        { "file": "./private/.ssh/", "link": "~/.ssh/" },
        { "file": "./private/git/symlink.gitconfig", "link": "~/.gitconfig" },
        { "file": "./private/sh/bin", "link": "~/.bin-private" },
        { "file": "./private/zsh/env.symlink.zsh", "link": "~/.env-private.zsh" },
        { "file": "./sh/bin", "link": "~/.bin" },
        {
          "file": "./sublime-text/user-package/",
          "link": "~/Library/Application Support/Sublime Text 3/Packages/User/"
        },
        { "file": "./zsh/aliases.symlink.zsh", "link": "~/.aliases.zsh" },
        { "file": "./zsh/env.symlink.zsh", "link": "~/.env.zsh" },
        { "file": "./zsh/zshrc.symlink.zsh", "link": "~/.zshrc" }
      ]
    },
    {
      "key": "write-defaults",
      "value": {
        "Apple Global Domain": {
          "AppleInterfaceStyle": "Dark",
          "AppleAccentColor": -1,
          "AppleHighlightColor": "0.847059 0.847059 0.862745 Graphite",
          "AppleAquaColorVariant": 6,
          "com.apple.trackpad.scaling": 3
        },
        "com.apple.AppleMultitouchTrackpad": {
          "Clicking": 1,
          "TrackpadCornerSecondaryClick": 2,
          "TrackpadRightClick": 0
        },
        "com.apple.driver.AppleBluetoothMultitouch.trackpad": {
          "Clicking": 1,
          "TrackpadCornerSecondaryClick": 2,
          "TrackpadRightClick": 0
        }
      }
    }
  ]
}