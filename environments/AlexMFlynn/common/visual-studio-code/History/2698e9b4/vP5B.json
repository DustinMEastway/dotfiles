{
  "fileRoot": "./environments/AlexMFlynn/",
  "commands": [
    {
      "key": "brew-install",
      "value": {
        "items": [
          "mono",
          "redis"
        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "args": "--cask",
        "futureItems": [
          "brave-browser",
          "discord",
          "docker",
          "godot-mono",
          "slack",
          "steam",
          "sublime-text",
          "visual-studio-code",
          "zoom"
        ],
        "items": [
          "altServer""iina",
          "insomnia",
          "linearmouse",
          "raycast",
          "sublime-merge"
        ]
      }
    },
    {
      "key": "git-setup",
      "value": {
        "configPath": "./private/git/symlink.gitconfig"
      }
    },
    {
      "key": "mkdir",
      "value": [
        "./private/.ssh/",
        "~/Library/Application Support/Sublime Merge/Packages/"
      ]
    },
    {
      "key": "npm-install",
      "value": {
        "packages": [
          "@angular/cli",
          "nx",
          "yarn"
        ]
      }
    },
    {
      "key": "ssh-keygen",
      "value": {
        "file": "./private/.ssh/id_rsa",
        "type": "rsa"
      }
    },
    {
      "key": "write-defaults",
      "value": {
        "Apple Global Domain": {
          "ApplePressAndHoldEnabled": false
        }
      }
    },
    {
      "key": "symlink",
      "value": [
        {
          "file": "./common/sh/aliases.symlink.sh",
          "link": "~/.aliases-common.sh"
        },
        {
          "file": "./common/sh/bashrc.symlink.sh",
          "link": "~/.bashrc"
        },
        {
          "file": "./common/sh/bin/",
          "link": "~/.bin-common/"
        },
        {
          "file": "./common/sh/common.symlink.sh",
          "link": "~/.common.sh"
        },
        {
          "file": "./common/sh/env.symlink.sh",
          "link": "~/.env-common.sh"
        },
        {
          "file": "./common/sh/zshrc.symlink.zsh",
          "link": "~/.zshrc"
        },
        {
          "file": "./common/sublime-merge/",
          "link": "~/Library/Application Support/Sublime Merge/Packages/User/"
