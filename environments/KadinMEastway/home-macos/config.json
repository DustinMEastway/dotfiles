{
  "fileRoot": "./environments/KadinMEastway/",
  "commands": [
    {
      "key": "brew-tap",
      "value": {
        "items": [
          "mongodb/brew"
        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "items": [
          "coreutils",
          "deno",
          "mongodb/brew/mongodb-community",
          "neovim",
          "node",
          "nvm",
          "openssl",
          "postgresql"
        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "args": "--cask",
        "items": [
          "1password",
          "1password-cli",
          "brave-browser",
          "curseforge",
          "discord",
          "docker",
          "gimp",
          "iina",
          "insomnia",
          "linearmouse",
          "microsoft-auto-update",
          "microsoft-office",
          "moonlight",
          "obsidian",
          "pgadmin4",
          "private-internet-access",
          "raycast",
          "slack",
          "steam",
          "visual-studio-code",
          "zed",
          "zoom"
        ]
      }
    },
    {
      "key": "brew-sync"
    },
    {
      "key": "git-setup",
      "value": {
        "configPath": "./private/git/symlink.gitconfig"
      }
    },
    {
      "key": "npm-install",
      "value": {
        "packages": [
          "nx"
        ]
      }
    },
    {
      "key": "ssh-keygen",
      "value": {
        "file": "./private/.ssh/id_rsa",
        "type": "rsa"
      }
    },
    {
      "key": "write-defaults",
      "value": {
        "Apple Global Domain": {
          "ApplePressAndHoldEnabled": false
        }
      }
    },
    {
      "key": "symlink",
      "value": [
        {
          "file": "./common/sh/aliases.symlink.sh",
          "link": "~/.aliases-common.sh"
        },
        {
          "file": "./common/sh/bashrc.symlink.sh",
          "link": "~/.bashrc"
        },
        {
          "file": "./common/sh/bin/",
          "link": "~/.bin-common/"
        },
        {
          "file": "./common/sh/common.symlink.sh",
          "link": "~/.common.sh"
        },
        {
          "file": "./common/sh/env.symlink.sh",
          "link": "~/.env-common.sh"
        },
        {
          "file": "./common/sh/zshrc.symlink.zsh",
          "link": "~/.zshrc"
        },
        {
          "file": "./home-macos/sh/aliases.symlink.sh",
          "link": "~/.aliases.sh"
        },
        {
          "file": "./home-macos/sh/bin/",
          "link": "~/.bin/"
        },
        {
          "file": "./home-macos/sh/env.symlink.sh",
          "link": "~/.env.template.sh"
        },
        {
          "file": "./private/.ssh/",
          "link": "~/.ssh/"
        },
        {
          "file": "./private/git/symlink.gitconfig",
          "link": "~/.gitconfig"
        },
        {
          "file": "./private/sh/env.symlink.sh",
          "link": "~/.env-private.sh"
        }
      ]
    },
    {
      "key": "git-clone",
      "value": {
        "items": [
          {
            "path": "~/Sites/brass-raven",
            "url": "git@github.com:brass-raven/brass-raven.git"
          }
        ]
      }
    }
  ]
}
