{
  "fileRoot": "./environments/KadinMEastway/",
  "commands": [
    {
      "key": "brew-tap",
      "value": {
        "items": [
          "homebrew/cask-versions",
          "mongodb/brew"
        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "items": [
          "coreutils",
          "mongodb-community",
          "redis"
        ]
      }
    },
    {
      "key": "brew-install",
      "value": {
        "args": "--cask",
        "items": [
          "brave-browser",
          "calibre",
          "curseforge",
          "discord",
          "gimp",
          "godot-mono",
          "iina",
          "insomnia",
          "multipatch",
          "private-internet-access",
          "raycast",
          "slack",
          "steam",
          "visual-studio-code",
          "zoom"
        ]
      }
    },
    {
      "key": "git-setup",
      "value": { "configPath": "./private/git/symlink.gitconfig" }
    },
    { "key": "mkdir", "value": "./private/.ssh/" },
    { "key": "mkdir", "value": "./private/sh/bin/" },
    {
      "key": "ssh-keygen",
      "value": {
        "file": "./private/.ssh/id_rsa",
        "type": "rsa"
      }
    },
    {
      "key": "symlink",
      "value": [
        { "file": "../..", "link": "~/.dotfiles" },
        { "file": "./common.symlink.sh", "link": "~/.common.sh" },
        { "file": "./private/.ssh/", "link": "~/.ssh/" },
        { "file": "./private/git/symlink.gitconfig", "link": "~/.gitconfig" },
        { "file": "./private/sh/bin", "link": "~/.bin-private" },
        { "file": "./private/zsh/env.symlink.zsh", "link": "~/.env-private.zsh" },
        { "file": "./sh/bin", "link": "~/.bin" },
        { "file": "./zsh/aliases.symlink.zsh", "link": "~/.aliases.zsh" },
        { "file": "./zsh/env.symlink.zsh", "link": "~/.env.zsh" },
        { "file": "./zsh/zshrc.symlink.zsh", "link": "~/.zshrc" }
      ]
    }
  ]
}
